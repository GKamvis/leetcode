class Solution {
  int? removeDuplicates(List<int> nums) {
    if (nums.length <= 2) return nums.length;
    int c = 2;
    for (int i = 2; i < nums.length; i++) {
      if (nums[i] != nums[c - 2]) {
        print(' burada i == olmadÄ± c-2 '
        );
        nums[c] = nums[i];
        c++;
      }
      else{
        print('burda oldu');
      }
    }
    return c;
  }
}
void main(){
  Solution sol = Solution();
  List<int> nums = [1,1,1,2,2,3];
  print(sol.removeDuplicates(nums));
}
